		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/svelte": "^5.0.0",
		"@types/node": "^22.0.0",
		"eslint": "^9.0.0",
		"eslint-plugin-svelte": "^2.0.0",
		"jsdom": "^25.0.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
